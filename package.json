{
  "name": "drum-front",
  "version": "1.0.0",
  "description": "Simple express server with sequelize, pg and jwts",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "env DELAY=1000 npx nodemon .",
    "initdev": "npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "resetDB": "npx sequelize-cli db:migrate:undo:all && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "template",
    "express",
    "jwt",
    "sequelize"
  ],
  "author": "Rein Op 't Land",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "node": "^17.7.2",
    "pg": "^8.7.1",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "axios": "^1.4.0",
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.6.1"
  }
}
